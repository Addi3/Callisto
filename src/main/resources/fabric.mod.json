{
	"schemaVersion": 1,
	"id": "callisto",
	"version": "${version}",
	"name": "Callisto",
	"description": "Name that comes from the Greek Kallisto. Translating to “most beautiful”.",
	"authors": [
		"Addie"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "ARR",
	"icon": "assets/callisto/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.lib.Callisto"
		],
		"client": [
			"com.lib.CallistoClient"
		],
		"fabric-datagen": [
			"com.lib.CallistoDataGenerator"
		]
	},
	"mixins": [
		"callisto.mixins.json",
		{
			"config": "callisto.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.18.3",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	}
}